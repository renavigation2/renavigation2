{
  "packages": [
    "examples/*",
    "packages/*"
  ],
  "ignoreChanges": [
    "**/package.json",
    "**/__tests__/**",
    "**/*.md",
    "**/Podfile.lock",
    "**/yarn.lock",
    "**/.vscode/**",
    "**/.vscode/**"
  ],
  "version": "4.22.2",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "version": {
      "allowBranch": [
        "master",
        "develop"
      ]
    }
  }
}
